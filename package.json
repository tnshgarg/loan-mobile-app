{
	"name": "employee-app",
	"version": "2023.5.5",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"lint": "eslint .",
		"start": "react-native start",
		"clean:start": " watchman watch-del-all && rm -rf node_modules && rm -rf /tmp/metro-* && npm add @babel/runtime && npm i && npx react-native start --reset-cache",
		"clean:gradle": "cd android && ./gradlew clean",
		"pod:install": "cd ios/ && pod install --repo-update && cd ..",
		"test": "jest",
		"postinstall": "patch-package",
		"android:start:dev": "gulp set --env=dev && npx react-native run-android --active-arch-only --variant=devDebug --appIdSuffix=dev",
		"android:start:qa": "gulp set --env=test && npx react-native run-android --active-arch-only --variant=qaDebug --appIdSuffix=qa",
		"android:start:prod": "gulp set --env=prod && npx react-native run-android --active-arch-only --variant=prodDebug --appIdSuffix=prod",
		"android:build:dev": "bash ./android/versioning.sh && gulp set --env=dev && cd android && ./gradlew assembleDevRelease",
		"android:build:qa": "bash ./android/versioning.sh && gulp set --env=test && cd android && ./gradlew assembleQaRelease",
		"android:build:demo": "bash ./android/versioning.sh && gulp set --env=test && cd android && ./gradlew assembleDemoRelease",
		"android:build:prod": "bash ./android/versioning.sh && gulp set --env=prod && cd android && ./gradlew assembleProdRelease",
		"ios:start:dev": "cp ios/GoogleService-Info-dev.plist ios/GoogleService-Info.plist && gulp set --env=dev && react-native run-ios --scheme 'employeeappDev'",
		"ios:start:qa": "cp ios/GoogleService-Info-qa.plist ios/GoogleService-Info.plist && cp ios/AppCenter-Config-Qa.plist ios/AppCenter-Config.plist && gulp set --env=test && react-native run-ios --scheme 'employeeappQa'",
		"ios:start:demo": "cp ios/GoogleService-Info-demo.plist ios/GoogleService-Info.plist && cp ios/AppCenter-Config-Demo.plist ios/AppCenter-Config.plist && gulp set --env=test && react-native run-ios --scheme 'employeeappDemo'",
		"ios:start:prod": "gulp set --env=prod && react-native run-ios --scheme 'employeeapp'",
		"android:codepush:qa": "appcenter codepush release-react -a Unipe/Unipe-New-QA -d QA -m",
		"ios:codepush:qa": "appcenter codepush release-react -a Unipe/Unipe-QA-App-iOS -d QA -m",
		"ios:test:deeplink": "npx uri-scheme open 'https://mobile-app.unipe.co/ewa' --ios",
		"android:test:deeplink": "npx uri-scheme open 'https://mobile-app.unipe.co/ewa' --android"
	},
	"dependencies": {
		"@miblanchard/react-native-slider": "^2.3.1",
		"@react-native-async-storage/async-storage": "^1.18.1",
		"@react-native-community/netinfo": "^9.3.10",
		"@react-native-firebase/analytics": "^17.4.3",
		"@react-native-firebase/app": "^17.5.0",
		"@react-native-firebase/crashlytics": "^17.5.0",
		"@react-native-firebase/in-app-messaging": "^17.4.3",
		"@react-native-firebase/messaging": "^17.4.3",
		"@react-native-firebase/remote-config": "^17.4.3",
		"@react-navigation/bottom-tabs": "^6.5.7",
		"@react-navigation/core": "^6.4.8",
		"@react-navigation/material-top-tabs": "^6.6.2",
		"@react-navigation/native": "^6.1.6",
		"@react-navigation/native-stack": "^6.9.12",
		"@reduxjs/toolkit": "^1.9.5",
		"appcenter": "4.4.5",
		"appcenter-analytics": "4.4.5",
		"appcenter-crashes": "4.4.5",
		"axios": "^1.4.0",
		"fuzzball": "^2.1.2",
		"gulp": "^4.0.2",
		"gulp-clean": "^0.4.0",
		"gulp-rename": "^2.0.0",
		"patch-package": "^7.0.0",
		"react": "18.2.0",
		"react-native": "0.71.7",
		"react-native-background-timer": "^2.4.1",
		"react-native-code-push": "github:microsoft/react-native-code-push#pull/2419/head",
		"react-native-device-info": "^10.6.0",
		"react-native-dotenv": "^3.4.8",
		"react-native-extended-stylesheet": "^0.12.0",
		"react-native-gesture-handler": "^2.9.0",
		"react-native-modal": "^13.0.1",
		"react-native-network-info": "^5.2.1",
		"react-native-pager-view": "^6.2.0",
		"react-native-popable": "^0.4.3",
		"react-native-pure-jwt": "^3.0.1",
		"react-native-razorpay": "^2.3.0",
		"react-native-reanimated": "^3.1.0",
		"react-native-render-html": "^6.3.4",
		"react-native-safe-area-context": "^4.5.2",
		"react-native-screens": "^3.20.0",
		"react-native-sms-retriever": "^1.1.1",
		"react-native-snackbar": "^2.6.2",
		"react-native-splash-screen": "^3.3.0",
		"react-native-step-indicator": "^1.0.3",
		"react-native-svg": "^13.9.0",
		"react-native-tab-view": "^3.5.1",
		"react-native-vector-icons": "^9.2.0",
		"react-native-webview": "^12.0.2",
		"react-native-youtube-iframe": "^2.2.2",
		"react-redux": "^8.0.5",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.8",
		"@babel/preset-env": "^7.21.5",
		"@babel/runtime": "^7.21.5",
		"@react-native-community/eslint-config": "^3.2.0",
		"@tsconfig/react-native": "^3.0.0",
		"@types/jest": "^29.5.1",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.5.0",
		"eslint": "^8.40.0",
		"jest": "^29.5.0",
		"metro-react-native-babel-preset": "0.76.3",
		"prettier": "^2.8.8",
		"react-native-set-version": "^1.1.2",
		"react-native-svg-transformer": "^1.0.0",
		"react-test-renderer": "18.2.0",
		"typescript": "5.0.4"
	},
	"resolutions": {
		"glob": "7.2.0"
	},
	"jest": {
		"preset": "react-native"
	}
}
